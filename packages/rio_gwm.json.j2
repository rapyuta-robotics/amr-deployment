{
  "name": "rio_gwm",
  "packageVersion": "v1.0.17",
  "description": "",
  "bindable": true,
  "plans": [
    {
      "name": "default",
      "metadata": {
        
      },
      "singleton": false,
      "components": [
        {
          "name": "core",
          "description": "",
          "cloudInfra": {
            "replicas": 1,
            "endpoints": [
              {
                "name": "GWM_CORE_URL",
                "exposeExternally": true,
                "port": 443,
                "targetPort": 8000,
                "proto": "HTTPS"
              }
            ]
          },
          "ros": {
            "topics": [
              
            ],
            "services": [
              
            ],
            "actions": [
              
            ],
            "isROS": false
          },
          "requiredRuntime": "cloud",
          "architecture": "amd64",
          "executables": [
            {
              "name": "core",
              "simulationOptions": {
                "simulation": false
              },
              {%- for key in source %}
                 "{{key}}": "{{ source[key] }}",
              {%- endfor %}
              "cmd": [
                "/bin/bash",
                "-c",
                "python manage.py runserver 0.0.0.0:8000"
              ],
              "limits": {
                "cpu": 0.5,
                "memory": 2048
              }
            }
          ],
          "parameters": [
            {
              "default": "true",
              "name": "DEBUG",
              "description": ""
            },
            {
              "default": "true",
              "name": "AUTOBOOTSTRAP",
              "description": ""
            },
            {
              "default": "autobootstrap",
              "name": "AUTO_ORG_NAME",
              "description": "Autocreated organisation name"
            },
            {
              "default": "autobootstrap",
              "name": "AUTO_ROOT_USER",
              "description": "Autocreated username"
            },
            {
              "default": "autobootstrap",
              "name": "AUTO_ROOT_PASS",
              "description": "Autocreated root password"
            },
            {
              "default": "autobootstrap",
              "name": "AUTO_AUTH_TOKEN",
              "description": "Autocreated initial token"
            }

          ]
        }
      ],
      "includePackages": [
        
      ],
      "dependentDeployments": [
        
      ],
      "inboundROSInterfaces": {
        "anyIncomingScopedOrTargetedRosConfig": false
      },
      "exposedParameters":[ 
        {"component":"core",
        "param":"GWM_AUTH_TOKEN",
        "targetParam":"AUTO_AUTH_TOKEN"
        }
      ]
    }
  ]
}